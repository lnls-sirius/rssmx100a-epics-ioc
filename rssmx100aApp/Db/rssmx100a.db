# CALC FOR FREQUENCY SWEEP MODE

record(mbbi, "$(P):$(R)GENERAL-FMode_RBV") {
  field(DESC, "Get Frequency Mode")
  field(SCAN, "1 second")
  field(ZRVL, "0")
  field(ZRST, "AUTO")
  field(ONVL, "1")
  field(ONST, "SINGLE")
  field(TWVL, "2")
  field(TWST, "STEP")
  field(THVL, "3")
  field(THST, "EXT SINGLE")
  field(FRVL, "4")
  field(FRST, "EXT STEP")
  field(FVVL, "5")
  field(FVST, "EXT START/STOP")
  field(INP, "$(P):$(R)GENERAL-CalcFMode")
}

record(calc, "$(P):$(R)GENERAL-CalcFMode") {
  field(DESC, "Calc for frequency mode")
  field(SCAN, "1 second")
  field(INPA, "$(P):$(R)FREQ-FSweepMode_RBV")
  field(INPB, "$(P):$(R)TRIG-FSweepSrc_RBV")
  field(CALC, "A=1?2:(A=2?4:(B=0?0:(B=1?1:(B=2?3:5))))")
  field(OUT, "$(P):$(R)GENERAL-FMode")
}
