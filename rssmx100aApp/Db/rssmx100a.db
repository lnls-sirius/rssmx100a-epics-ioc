##########################################################################
# Reset to default state

record(bo, "$(P):$(R)GENERAL-Reset") {
  field(DESC, "Reset to default state")
  field(DTYP, "stream")
  field(ZNAM, "0")
  field(ONAM, "1")
  field(OUT, "@rssmx100a.proto outRst $(PORT)")
}
##########################################################################
# Get identification

